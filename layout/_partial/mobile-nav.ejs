<nav id="mobile-nav">
  <% for (var i in theme.menu){ %>
    <a href="<%- url_for(theme.menu[i]) %>" class="mobile-nav-link">
      <%= i %>
    </a>
    <% } %>
      <% if (theme.rss){ %>
        <a id="nav-rss-link" class="mobile-nav-link" href="<%- url_for(theme.rss) %>"
          title="<%= __('rss_feed') %>">RSS</a>
        <% } %>
</nav>