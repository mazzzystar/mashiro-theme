<% if (typeof post !=='undefined' ){ %>
  <% if (post.link){ %>
    <h3 itemprop="name">
      <a class="<%= class_name %>" href="<%- url_for(post.link, {relative: false}) %>" target="_blank" itemprop="url">
        <%= post.title %>
      </a>
    </h3>
    <% } else if (post.title){ %>
      <% if (index){ %>
        <h3 itemprop="name">
          <a class="<%= class_name %>" href="<%- url_for(post.path) %>">
            <%= post.title %>
          </a>
        </h3>
        <% } else { %>
          <h2 class="<%= class_name %>" itemprop="headline name">
            <%= post.title %>
          </h2>
          <% } %>
            <% } %>
              <% } %>